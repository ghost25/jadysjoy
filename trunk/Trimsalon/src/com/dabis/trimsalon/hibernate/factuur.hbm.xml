<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dabis.trimsalon.beans">
  <class name="Factuur" table="APP.FACTUUR">
     <id name="id" column="ID">
            <generator class="native"/>
     </id>
	<set name="boekhouding">
		<key column="ID" not-null="true"/>
		<one-to-many class="Boekhouding"/>
	</set>
	<property name="factuurdatum" column="FACTUURDATUM" type="calendar_date" />
	<property name="factuurnummer" column="FACTUURNUMMER" length="10" />
  </class>
</hibernate-mapping>